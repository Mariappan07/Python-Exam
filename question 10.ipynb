    "#10.displaying the years from dates\n",
    "import datetime\n",
    "date = ['17-12-1997','22-04-2011','01-05-1993','19-06-2020']\n",
    "l=[]\n",
    "for i in date:\n",
    "    datem = datetime.datetime.strptime(i, \"%d-%m-%Y\")\n",
    "    l.append(str(datem.year))\n",
    "print(l)"
   ]
  }
